<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title> pdf generator</title>
</head>

<body>
  <div class="container">
    <h1 class="text-center">
    </h1>
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Age</th>
          <th>Gender</th>
          <th>Email</th>
          <th>phone</th>
          <th>address</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(user) { %>
          <tr>
            <td>
              <%= user.name %>
            </td>
            <td>
              <%= user.age %>
            </td>
            <td>
              <%= user.gender %>
            </td>
            <td>
              <%= user.email %>
            </td>
            <td>
              <%= user.phone %>
            </td>
            <td>
            <td>
              <%= user.address.street %>, <%= user.address.city %>, <%= user.address.state %>
                    <%= user.address.zip %>
            </td>
            </td>
          </tr>
          <% }); %>
      </tbody>
    </table>
  </div>
</body>

</html>